{
  "manifest_version": 3,
  "name": "Tab Russian Roulette",
  "version": "1.1.7",
  "description": "Randomly closes a tab when you click the button.",
  "permissions": ["tabs"],
  "action": {
    "default_popup": "popup.html",
    "default_icon": {
      "128": "icon.png"
    }
  },
  "background": {
    "service_worker": "background.js"
  },
  "icons": {
    "128": "icon.png"
  }
}